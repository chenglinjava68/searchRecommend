log4j.rootLogger=INFO,CONSOLE,esAppender
log4j.appender.CONSOLE=org.apache.log4j.ConsoleAppender
log4j.appender.Threshold=DEBUG
log4j.appender.CONSOLE.Target=System.out
log4j.appender.CONSOLE.layout=org.apache.log4j.PatternLayout
log4j.appender.CONSOLE.layout.ConversionPattern=[baihe-searchRecommend\:]%d{yyyy-MM-dd HH\:mm\:ss,SSS} %r %t %l %m%n
log4j.appender.esAppender=org.apache.log4j.net.SocketAppender
log4j.appender.esAppender.port=9991
log4j.appender.esAppender.remoteHost=172.16.11.43

log4j.category.org.springframework = ERROR
log4j.logger.org.apache.zookeeper =ERROR

log4j.category.com.baihe=DEBUG

